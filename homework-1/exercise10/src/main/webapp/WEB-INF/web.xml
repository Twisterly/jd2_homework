<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <display-name>Form for name, telephone number and email</display-name>


    <servlet>
        <servlet-name>PrintFormServlet</servlet-name>
        <servlet-class>com.init.servlets.PrintFormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrintFormServlet</servlet-name>
        <url-pattern>/print-form</url-pattern>
    </servlet-mapping>

</web-app>
